<form action="updated" method="post">

<%= label_tag 'change' %>
<%= radio_button_tag 'change', 'new'%> new <br>
<%= radio_button_tag 'change', 'update'%> update <br>
<%= radio_button_tag 'change', 'remove'%> remove <br>

<%= label_tag 'Experiment Name'%> 
<%= text_field_tag 'experimentName', @expOb['experimentName'],:placeholder => 'enter experiment\'s name' %>
</br>
<% @expOb['sequence'].each_index do |t|%>
<%= select 'sequence', nil,  @sequenceNames,{:prompt=>'select sequence', :selected => @expOb['sequence'][t]}%> <br>
<%end%>
</br>

<input type =submit value="update!">

</form>
